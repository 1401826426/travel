<?xml version="1.0" encoding="UTF-8"?>
<bean>
    <bean name="nettyConf">
        <property name="port">8081</property>
        <property name="sessionInvalidTime">1800000</property>
        <property name="sessionCheckInterval">60000</property>
    </bean>
    <bean name="springConf">
       <property name="springPath">classpath:spring/spring-*.xml</property>
        <property name="springMvcPath">classpath:spring/servlet.xml</property>
    </bean>
</bean>
